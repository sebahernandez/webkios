data);\n  };\n\n  useEffect(() => {\n    setTimeout(() => {\n      window.scrollTo(0, 0);\n    }, 500);\n  }, []);\n\n  return (\n    <>\n      <ProductDetailsWrapper className=\"product-card\" dir=\"ltr\">\n        {!isRtl && (\n          <ProductPreview>\n            <BackButton>\n              <Button\n                type=\"button\"\n                size=\"small\"\n                style={{\n                  backgroundColor: '#ffffff',\n                  border: '1px solid #f1f1f1',\n                  color: '#77798c',\n                }}\n                onClick={Router.back}\n              >\n                <LongArrowLeft style={{ marginRight: 5 }} />\n                <FormattedMessage id=\"backBtn\" defaultMessage=\"Back\" />\n              </Button>\n            </BackButton>\n\n            <CarouselWithCustomDots\n              items={product.gallery}\n              deviceType={deviceType}\n            />\n          </ProductPreview>\n        )}\n\n        <ProductInfo dir={isRtl ? 'rtl' : 'ltr'}>\n          <ProductTitlePriceWrapper>\n            <ProductTitle>{product.title}</ProductTitle>\n            <ProductPriceWrapper>\n              {product.discountInPercent ? (\n                <SalePrice>\n                  {CURRENCY}\n                  {product.price}\n                </SalePrice>\n              ) : null}\n\n              <ProductPrice>\n                {CURRENCY}\n                {product.salePrice ? product.salePrice : product.price}\n              </ProductPrice>\n            </ProductPriceWrapper>\n          </ProductTitlePriceWrapper>\n\n          <ProductWeight>{product.unit}</ProductWeight>\n          <ProductDescription>\n            <ReadMore character={600}>{product.description}</ReadMore>\n          </ProductDescription>\n\n          <ProductCartWrapper>\n            <ProductCartBtn>\n              {!isInCart(data.id) ? (\n                <Button\n                  className=\"cart-button\"\n                  variant=\"secondary\"\n                  borderRadius={100}\n                  onClick={handleAddClick}\n                >\n                  <CartIcon mr={2} />\n                  <ButtonText>\n                    <FormattedMessage\n                      id=\"addCartButton\"\n                      defaultMessage=\"Cart\"\n                    />\n                  </ButtonText>\n                </Button>\n              ) : (\n                <Counter\n                  value={getItem(data.id).quantity}\n                  onDecrement={handleRemoveClick}\n                  onIncrement={handleAddClick}\n                />\n              )}\n            </ProductCartBtn>\n          </ProductCartWrapper>\n\n          <ProductMeta>\n            <MetaSingle>\n              {product?.categories?.map((item: any) => (\n                <Link\n                  href={`/${product.type.toLowerCase()}?category=${item.slug}`}\n                  key={`link-${item.id}`}\n                >\n                  <a>\n                    <MetaItem>{item.title}</MetaItem>\n                  </a>\n                </Link>\n              ))}\n            </MetaSingle>\n          </ProductMeta>\n        </ProductInfo>\n\n        {isRtl && (\n          <ProductPreview>\n            <BackButton>\n              <Button\n                title=\"Back\"\n                intlButtonId=\"backBtn\"\n                iconPosition=\"left\"\n                size=\"small\"\n                style={{\n                  backgroundColor: '#ffffff',\n                  border: '1px solid #f1f1f1',\n                  color: '#77798c',\n                }}\n                icon={<LongArrowLeft />}\n                onClick={Router.back}\n              />\n            </BackButton>\n\n            <CarouselWithCustomDots\n              items={product.gallery}\n              deviceType={deviceType}\n            />\n          </ProductPreview>\n        )}\n      </ProductDetailsWrapper>\n\n      <RelatedItems>\n        <h2>\n          <FormattedMessage\n            id=\"intlRelatedItems\"\n            defaultMessage=\"Related Items\"\n          />\n        </h2>\n        <Products\n          type={product.type.toLowerCase()}\n          deviceType={deviceType}\n          loadMore={false}\n          fetchLimit={10}\n        />\n      </RelatedItems>\n    </>\n  );\n};\n\nexport default ProductDetails;\n"]},"metadata":{},"sourceType":"module"}