oCart?: any;\n  updateCart?: any;\n  value?: any;\n  deviceType?: any;\n};\n\nconst ProductCard: React.FC<ProductCardProps> = ({\n  title,\n  image,\n  weight,\n  price,\n  salePrice,\n  discountInPercent,\n  cartProducts,\n  addToCart,\n  updateCart,\n  value,\n  currency,\n  onChange,\n  increment,\n  decrement,\n  data,\n  deviceType,\n  ...props\n}) => {\n  const router = useRouter();\n\n  // modal del producto\n  const [showModal, hideModal] = useModal(\n    () => (\n      <QuickViewMobile\n        modalProps={data}\n        hideModal={hideModal}\n        deviceType={deviceType}\n      />\n    ),\n    {\n      onClose: () => {\n        const { pathname, query, asPath } = router;\n        const as = asPath;\n        router.push(\n          {\n            pathname,\n            query,\n          },\n          as,\n          {\n            shallow: true,\n          }\n        );\n      },\n    }\n  );\n  const { addItem, removeItem, getItem, isInCart } = useCart();\n  const handleAddClick = (e) => {\n    e.stopPropagation();\n    addItem(data);\n    if (!isInCart(data.id)) {\n      cartAnimation(e);\n    }\n  };\n  const handleRemoveClick = (e) => {\n    e.stopPropagation();\n    removeItem(data);\n  };\n  const handleQuickViewModal = () => {\n    const { pathname, query } = router;\n    const as = `/product/${data.slug}`;\n    if (pathname === '/product/[slug]') {\n      router.push(pathname, as);\n      if (typeof window !== 'undefined') {\n        window.scrollTo(0, 0);\n      }\n      return;\n    }\n    showModal();\n    router.push(\n      {\n        pathname,\n        query,\n      },\n      {\n        pathname: as,\n      },\n      {\n        shallow: true,\n      }\n    );\n  };\n\n  return (\n    <ProductCardWrapper onClick={handleQuickViewModal} className=\"product-card\">\n      <ProductImageWrapper>\n        <Image\n          url={image}\n          className=\"product-image\"\n          style={{ position: 'relative' }}\n          alt={title}\n        